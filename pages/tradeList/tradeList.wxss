.list-container{
  position: relative
}
.search-input{
  padding: 16rpx;
  position: relative;
  background: #F8F8F8;
}
.list-input{
  height: 68rpx;
  border: 1px solid #E6E6EA;
  border-radius: 10rpx;
  padding: 0 60rpx;
  background: #ffffff
}
.clear{
  position: absolute;
  top: 38rpx;
  right: 30rpx;
  z-index: 99
}
.nameP{
  display: inline-block;
  width: 76%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}
.search{
  position: absolute;
  top: 38rpx;
  left: 30rpx;
  z-index: 99
}
.country-choose{
  position: relative;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  padding: 0 30rpx;
  font-size: 34rpx;
  color: #222222;
  border-top: 1px solid  #E5E5E5;
  border-bottom: 1px solid  #E5E5E5;
  box-sizing: border-box
}
.arrow{
  position: absolute;
  right: 32rpx;
  top: 32rpx;
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  border: 4rpx solid #C7C7CC;
  transform: rotate(50deg);
  border-bottom: none;
  border-left: none;
}
.contect{
  width: 100%;
  height: 186rpx;
  padding: 16rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #E5E5E5; 
}
.name{
  font-size: 34rpx;
  color: #1677F7;
}
.time{
  float: right;
  font-size: 26rpx;
  color: #999999;
}
.list-contect{
  font-size: 26rpx;
  color: #999999;
  width: 100%;
  min-height: 18rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-top: 16rpx;
  white-space: nowrap;
}
.country-name{
  font-size: 26rpx;
  color: #999999;
  margin-top: 16rpx;
}
.tab-country{
  width: 100%;
  height: 1334rpx;
  position: fixed;
  bottom: 0;
  /* opacity: 0.5; */
  z-index: 99;
  /* background: #999999 */
}
.picker-country{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 500rpx;
  background: #fff;
  color: #222222
}
.sign-country{
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}