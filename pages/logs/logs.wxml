<!--logs.wxml-->
<image src='http://img.souche.com/f2e/ad1809c6441823cdbb9379c639e1c0bc.png'></image>
<view class="log-list">
  <view class='login-view'><view class='loger'>手机： </view><input class="login-input bn" bindinput='userInput' placeholder='请输入账号'></input></view>
  <view class='login-view'><view class='loger'>密码： </view><input class='login-input bn' bindinput='psdInput' placeholder='请输入密码' password="true"></input></view>
</view>
<view class='reg' bindtap='regTo'>没有账户，注册一下吧。</view>
<view class="{{userInput !== '' && psdInput !== ''?'log-to gb':'log-to bg'}}" bindtap='login'>登录</view>
